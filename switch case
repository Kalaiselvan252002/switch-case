#include<stdio.h>
int main()
{
    int x;
    printf("Enter any one number between 1 to 5\n");
    scanf("%d",&x);
    switch(x)
    {
    case 1:
        printf("Food item - Briyani.\nPrice =120.");
        break;
    case 2:
        printf("Food item - Saambar rice.\nPrice =60.");
        break;
    case 3:
        printf("Food item - Curd rice.\nPrice =40.");
        break;
    case 4:
        printf("Food item - Full meals.\nPrice =70.");
        break;
    case 5:
        printf("Food item - Coconut rice.\nPrice =40.");
    }
    return 0;
}
